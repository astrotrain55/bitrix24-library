    "prettier": "prettier . --write"
  },
  "dependencies": {
    "is-mobile": "^4.0.0"
  },
  "devDependencies": {
    "prettier": "3.2.4",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  }
